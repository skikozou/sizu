{
  "functions": {
    "api/curl.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/curl?path=$1",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*curl.*"
        }
      ]
    }
  ]
}
